## DO NOT EDIT THIS FILE, USE nuxeo.conf  ##

# Apache Kafka servers list
nuxeo.kafka.core.event.servers=localhost:9092

# Apache Kafka value serializer
nuxeo.kafka.core.event.serializer=org.apache.kafka.common.serialization.StringSerializer

# Apache Kafka value deserializer
nuxeo.kafka.core.event.deserializer=org.apache.kafka.common.serialization.StringDeserializer